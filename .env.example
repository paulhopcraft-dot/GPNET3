# GPNet3 Environment Configuration
# Copy this file to .env and fill in your values

# ===================
# Database (Required)
# ===================
DATABASE_URL=postgresql://postgres:postgres@localhost:5432/gpnet3

# ===================
# Authentication (Required)
# ===================
JWT_SECRET=your-secure-jwt-secret-minimum-32-characters

# ===================
# AI Services (Optional)
# ===================
# Anthropic API key for AI summaries and compliance assistant
ANTHROPIC_API_KEY=

# ===================
# Freshdesk Integration (Optional)
# ===================
FRESHDESK_DOMAIN=
FRESHDESK_API_KEY=

# ===================
# Application Settings
# ===================
NODE_ENV=development
PORT=5000
